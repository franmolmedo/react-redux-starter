{
  "name": "react-redux-starter",
  "version": "1.0.0",
  "description": "React - Redux - React Router - Asp.net Core bootstrapping project for Frontend Apps",
  "scripts": {
    "test": "set NODE_ENV=test&& mocha --compilers js:babel-core/register,js:./helpers/css-modules-compiler.js --require babel-polyfill app/features --recursive",
    "test:watch": "yarn test -- --watch",
    "test:cover": "set NODE_ENV=test&& rimraf coverage && istanbul cover \"node_modules/mocha/bin/_mocha\" -- --compilers js:babel-core/register,js:./helpers/css-modules-compiler.js --require babel-polyfill app/features --recursive",
    "lint": "eslint . --ext .js --ext .jsx --cache",
    "start": "yarn dll && yarn start:nodep",
    "start:nodep": "node webpack-dev-server.js",
    "dll": "webpack --config webpack.dll.js",
    "build": "rimraf build && yarn lint && yarn test && webpack --config ./webpack.config.prod.js --progress --optimize-minimize --optimize-occurrence-order && cd server && dotnet publish project.json  -c Release -o ../build"
  },
  "repository": {
    "type": "git",
    "url": "https://plainconcepts.visualstudio.com/DefaultCollection/PlainConcepts.Foils.Web/_git/ReactReduxStarter"
  },
